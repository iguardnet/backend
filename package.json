{
  "name": "optinetflow",
  "version": "0.0.1",
  "description": "OptiNetFlow",
  "author": "Masih Jahangiri",
  "license": "MIT",
  "keywords": [
    "OptiNetFlow"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/masihjahangiri/OptiNetFlow.git"
  },
  "bugs": {
    "url": "https://github.com/masihjahangiri/OptiNetFlow/issues"
  },
  "packageManager": "pnpm@8.6.11",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@devoxa/prisma-relay-cursor-connection": "2.2.3",
    "@nestjs/apollo": "12.0.7",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/graphql": "12.0.8",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.1.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "7.1.2",
    "@types/cookie": "^0.5.2",
    "axios": "^1.6.8",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "firebase-admin": "^12.3.0",
    "graphql": "16.7.1",
    "graphql-scalars": "1.22.2",
    "graphql-subscriptions": "2.0.0",
    "graphql-upload": "14.0.0",
    "mime-types": "^2.1.35",
    "minio": "^7.1.1",
    "nanoid": "^3.1.23",
    "nestjs-prisma": "0.22.0-dev.0",
    "nestjs-telegraf": "^2.7.0",
    "p-queue": "^6.6.2",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sharp": "^0.33.0",
    "shelljs": "^0.8.5",
    "telegraf": "^4.15.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.10",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.0",
    "@prisma/client": "^5.2.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.3.1",
    "@types/shelljs": "^0.8.12",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/eslint-plugin-tslint": "^6.2.1",
    "@typescript-eslint/parser": "^5.59.11",
    "chance": "1.1.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "29.6.1",
    "prettier": "2.8.8",
    "prisma": "5.2.0",
    "prisma-dbml-generator": "0.12.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
